<p id="notice"><%= notice %></p>

<h1>Dailylogs</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Daily quotes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dailylogs.each do |dailylog| %>
      <tr>
        <td><%= dailylog.description %></td>
        <td><%= dailylog.daily_quotes %></td>
        <td><%= dailylog.created_at.strftime("%Y-%m-%d %H-%M-%S") %></td>
        <td><%= link_to 'Show', dailylog %></td>
        <td><%#= link_to 'Edit', edit_dailylog_path(dailylog) %></td>
        <td><%#= link_to 'Destroy', dailylog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dailylog', new_dailylog_path %>
